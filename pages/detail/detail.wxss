/* pages/detail/detail.wxss */
/* title-color */
.loadIn{
  pointer-events: none;
}
.loadOut{
  pointer-events: auto; 
}
.yes-check-in{
  color: #606060;
}
.no-check-in{
  color: #15be49;
}
.hide { 
 display: none !important; 
} 
.show{ 
 display: block !important;
} 
.now-location{
  position:absolute;
  top: 170rpx;
  left: 10rpx;
  width:100rpx;
  height:100rpx;
  z-index:100000;
}
.bottom{
  width: 100%;
  position: fixed;
  height: 100rpx;
  background: #fff;
  z-index:10000;
  bottom: 0;
}
.clear{
  display:block;
  clear:both;
}
.one{
  background: #ff9439;
}
.two{
  background:rgb(227, 99, 72);
}
.three{
  background:rgb(58, 199, 219);
}
.four{
  background:rgb(71, 203, 143);
}
.five{
  background:rgb(118, 167, 207);
}
.six{
  background: rgb(249, 138, 180);  
}
.tag-color{
  width: 45%;
  height: 45rpx;
  position: absolute;
  z-index: 1;
}
.btm-one{
  border-bottom: 1px solid #eee;
}
/*轮播图  */
.warp{
  background: rgb(242, 242, 242);
  height:100%;
}

.banner {
   height: 450rpx; 
}

.slide-image {
  width: 100%; 
  height: 100%;
}
/* 轮播下角标 */
.page-index{
  display: block;
  width: 80rpx;
  font-size: 25rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align:center;
  background:rgba(0,0,0,.3);
  color: #fff;
  position: absolute;
  right: 30rpx;
  bottom:30px;
  border-radius:20rpx;
}
/* 房间详情 */
.room-d {
  width:100%;
  background: #fff;
  margin:0 auto;
  height:180rpx;
  margin-top:20rpx; 
}
.room-detail{
  width:100%;
  background: #fff;
  margin:0 auto;
  padding-bottom: 8rpx;
}
.room-title{
  width: 95%;
  margin: 0 auto;
  height: 80rpx;
  line-height: 80rpx;
}
.pirce{
  display:block;
  font-weight: 700;
  float: left;
  margin-left:30rpx;
  color: #df4d4e;
  font-size: 40rpx;
}
.collect{
    float: right;
    text-align: center;
    font-size: 30rpx;
    margin-right: 30rpx;
    position: relative;
}
.xcollect{    
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 25rpx;
  left: 50%;
  margin-left: -15rpx;
}
.tcollect{
  display: block;
  margin-top: 40rpx;
}
.room-cdetail {
    font-size:30rpx;
    margin:-10rpx 55rpx;
    position:absolute;
}
.room-tag{
  padding-left:30rpx;
  width:90%;
  margin:48rpx auto 0;
  border-top:1px solid #eee;
  padding-top:15rpx;
}
.tag-name{
    display: block;
    float: left;
    height:33rpx;
    font-size:24rpx;
    line-height:22rpx;
    /* color: #f9ffff; */
    border-radius: 5rpx;
    text-align: center;
    margin-right:15rpx;
    padding: 5rpx 8rpx 0;
    margin-bottom: 10rpx;
    /* background: #99bcd8; */
} 
/* .tag-name:nth-child(even){
   background:#e99064; 
}  */
.tag-name:nth-child(odd){
  border:1px solid #E78555;
  color: #E78555;
}
.tag-name:nth-child(even){
  border:1px solid #84B1D4;
  color: #84B1D4;
}
.tag-name:nth-child(3n+3){
  border:1px solid #7EB996 !important;
  color: #7EB996 !important;
}
.row-col{
  display:block;
  float: left;
  width:50%;
  height:55rpx;
  line-height:55rpx;
  font-size:30rpx;
}
.row-col .row{
  color: #939393;
}
.details{
  width:85%;
  margin:0 auto;
  padding-top:10rpx;
  height:105rpx;
}
.phone{
  height:55rpx;
  width:90%;
  margin:8rpx auto 0;
  border-top:1px solid #eee;
  padding-top:2rpx;
  position:relative;
}
.phone-name{
  font-size: 30rpx;
  color: #2a2a2a;
  text-align: center;
  box-sizing: border-box;
  display:block;
  height:50rpx;
  line-height: 50rpx;
  text-indent: 30rpx;
}
.tel{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  left:13%;
  top:9rpx;
}
.phone-num{
   font-size: 35rpx;
   color: #e94e5c;
}
/* houseRoomDetail */
.houseRoomDetail{
  font-size: 30rpx;
  width: 96%;
  margin: 0 auto;
  margin-top: 20rpx;
  background-color: #fff;
}
.houseRoomDetail-title{
  width: 100%;
  height: 65rpx;
  line-height: 65rpx;
  position: relative;
  text-indent: 30rpx;
  border-bottom: 8rpx solid rgb(242, 242, 242);
}
.houseRoomDetail-color{
  width:8rpx;
  position: absolute;
  height: 50rpx;
  top:50%;
  left: 0;
  margin-top: -25rpx;
}
/* 房屋配套 */
.house-content {
  text-indent: 50rpx;
  padding: 30rpx;
  font-size: 25rpx;
  position: relative;
  border-bottom: 20rpx solid rgb(242, 242, 242);
}
.facility{
  padding:10rpx;
  border-bottom:20rpx solid #eee;
}
.room-items{
  height: 50rpx;
  line-height: 50rpx;
}
.room-id{
  float: left;
  font-weight: 700;
  color: #fff;
  text-indent: 30rpx;
}
.text{
  position: absolute;
  z-index: 2;
}
.room-area{
  float: left;
  font-size:24rpx;
  color: #fff;
}
/* facility-content */
.facility-content{
  padding: 20rpx;
}
.facility-col{
  width: 33.33%;
  height:50rpx;
  display: inline-block;
  font-size:25rpx;
  position: relative;
}
.facility-img{
  width: 30rpx;
  position: absolute;
  margin-top: 10rpx;
  height: 30rpx;
}
.facility-text{
  text-indent:40rpx;
  position: absolute;
  top:8rpx;
}
/* 合租人 */
.room-mate {
  border-bottom:19rpx solid #eee;
}
.room-mate-li{
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  border-bottom:1px solid #eee;
}
.room-mate-li1{
  float: left;
  width: 15%;
  height: 90rpx;
  position: relative;
}

.room-mate-li1 span{
  width: 50rpx;
  height: 50rpx;
  line-height:50rpx;
  color: #fff;
  font-weight:bold; 
  position: absolute;
  border-radius: 50%;
  background: #ff943a;
  top: 50%;
  left: 65%;
  margin-top:-25rpx; 
  text-align: center;
}
.room-mate-li:nth-child(odd) .room-mate-li1 span{
  background: #ff943a;
}
.room-mate-li:nth-child(even) .room-mate-li1 span{
  background: #e2614c;
}
.room-mate-li:nth-child(3n+3) .room-mate-li1 span{
  background: #3cc4dc;
}
.room-mate-li2{
  float: left;
  width: 30%;
  text-align: center;
  font-size: 25rpx;
}
.room-mate-li3{
  float: left;
  width: 30%;
  text-indent:5rpx;
  font-size: 25rpx;
  color: #d31a20;
  position: relative;
}
.room-mate-li3-img{
  width:40rpx;
  height:40rpx;
  position: absolute;
  right: -5rpx;
  top:50%;
  margin-top:-20rpx;
}
.room-mate-li4{
  float: left;
  width: 25%;
  text-indent:20rpx;
  font-size: 25rpx;
  color: #d31a20;
  font-weight:bold;
}
.room-mate-li5{
  float: left;
  width: 25%;
  text-indent:20rpx;    
  color: #289cc3;
  text-decoration: underline;
  font-size: 25rpx;
  font-weight:bold;
}
/* 服务 */
.room-serve{
  width: 100%;
  border-bottom:20rpx solid #eee;
}
.room-server-li{
  height: 80rpx;
  line-height: 80rpx;
  width: 96%;
  margin:0 auto;
  border-bottom:1px solid #eee;
  position: relative;
  font-size:25rpx;
}
.room-server-img{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 50%;
  margin-top: -15rpx;
}
.room-server-text{
  display: block;
  text-indent: 40rpx;
}
.room-server-text2{
  position:absolute;
  text-indent:40rpx;
  top:50%;
  margin-top:-40rpx;
  left:13%;
}
/* room-round */
.room-wrap{
  position: relative;
  padding:10rpx;
  border-bottom: 20rpx solid #eee;
}
.room-round{
  position: relative;
}
.room-round-img{
  float: left;
  width: 30rpx;
  height: 38rpx;
}
/* 周边 */
.room-round-text{
  float: left;
  margin-left: 15rpx;
  color: rgb(241, 151, 65);
}
/* 交通 */
.room-transport-text{
  float: left;
  margin-left: 15rpx;
  color: rgb(227, 97, 97);
}
.room-round-space{
  width: 100%;
}
.room-round-text2{
  display: block;
  font-size: 25rpx;
  text-indent: 50rpx;
  padding: 15rpx;
  left: 50rpx;
}
/* room-district-text */
.room-district{
  border-bottom: 20rpx solid #eee;
}
.room-district-text{
  font-size: 25rpx;
  text-indent: 50rpx;
  display: block;
  width: 88%;
  margin: 0 auto;
  margin-top: 20rpx;
  padding:20rpx;
  border-top: 1px solid #eee;
}
/* 国安家推荐 */
.recommend{
  margin-top: 30rpx;
  width: 100%;
  padding-bottom:20rpx; 
  border-bottom:20rpx solid #eee; 
}
.recommend-title{
  width: 100%;
  text-align: center;
  line-height: 60rpx;
  color: #e24e5a;
  height: 60rpx;
  font-size: 45rpx;
  margin-bottom: 20rpx;
}
.recommend-title p{
    display: inline-block;
    height: 4rpx;
    width: 130rpx;
    background: #e8495b;
    margin-bottom: 8rpx;
}
.recommend-title text{
    display: inline-block;
    margin: 0 20rpx;
}
/* money */
.room-money{
  width: 100%;
}
.room-money-w{
  width: 33.33%;
  position: relative;
  height: 90rpx;
  line-height: 90rpx;
  float: left;
}
.room-money-money{
  font-size:37rpx;
  text-indent: 30rpx;
  text-align: center;
  color: #df4d4e;
  font-weight: 700;
}
.botton{
  position: absolute;
  top: 50%;
  left: 50%;
  margin-top: -22.5rpx;
  margin-left:-100rpx; 
  width: 150rpx;
  height: 60rpx;
  font-size: 25rpx;
  line-height: 60rpx;
  border-radius: 15rpx;
  text-align: center;
  color: #fff;
}
.look-about{
  margin-left:-75rpx; 
  background: #f6b14c;
}
.room-down{
  background: #e2614c;
}